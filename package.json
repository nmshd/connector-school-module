    },
    "devDependencies": {
        "@js-soft/docdb-access-mongo": "^1.3.1",
        "@js-soft/eslint-config-ts": "^2.0.4",
        "@js-soft/license-check": "^1.0.10",
        "@js-soft/ts-utils": "^2.3.5",
