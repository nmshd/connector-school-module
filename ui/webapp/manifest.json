{
    "_version": "1.1.0",
    "sap.app": {
        "id": "eu.enmeshed.connectorui",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "${version}"
        },
        "resources": "resources.json"
    },
    "sap.ui": {
        "fullWidth": true,
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://detail-view",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "eu.enmeshed.connectorui.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.90.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {},
                "sap.ui.layout": {},
                "sap.ui.table": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "eu.enmeshed.connectorui.i18n.i18n",
                    "supportedLocales": ["en", "de"],
                    "fallbackLocale": "en",
                    "async": true
                }
            },
            "apiKey": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "config": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "style/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "viewPath": "eu.enmeshed.connectorui.view",
                "controlId": "fcl",
                "controlAggregation": "beginColumnPages",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "login",
                    "target": ["login"]
                },
                {
                    "pattern": "master/:layout:",
                    "name": "master",
                    "target": ["master"]
                },
                {
                    "pattern": "detail/{id}/{layout}",
                    "name": "detail",
                    "target": ["master", "detail"]
                }
            ],
            "targets": {
                "login": {
                    "viewName": "Login",
                    "viewLevel": 1,
                    "viewId": "login"
                },
                "master": {
                    "viewName": "Master",
                    "viewLevel": 1,
                    "viewId": "master"
                },
                "detail": {
                    "viewName": "Detail",
                    "viewId": "detail",
                    "viewLevel": 1,
                    "controlAggregation": "midColumnPages"
                },
                "detailObjectNotFound": {
                    "viewName": "DetailObjectNotFound",
                    "viewId": "detailObjectNotFound",
                    "controlAggregation": "midColumnPages"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                }
            }
        }
    }
}
