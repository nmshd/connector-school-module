<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
>
    <Dialog
        id="addStudentsDialog"
        title="Add Students"
    >
        <content>
            <u:FileUploader
                id="csvFileUploader"
                name="myCSVFileUpload"
                change=".onCSVFileChanged"
                tooltip="Upload your csv entry"
            />

        </content>
        <beginButton>
            <Button
                text="Close"
                press=".onCloseAddStudentsDialog"
            />
        </beginButton>
        <endButton>
            <Button
                text="Sichern"
                press=".onUploadFiles"
            />
        </endButton>
    </Dialog>
</core:FragmentDefinition>